record(ai, "$(P)sDoseRt") {
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP, "@pdsgo2.proto get_dose-rate($(P)sErrStr) $(PORT)")
    field(ASLO, "0.01")
    field(EGU, "µSv/h")
    field(PREC, "1")
    field(ADEL, "0.2")
    field(HIGH, "2.0")
    field(HSV, "MINOR")
    field(HIHI, "10.0")
    field(HHSV, "MAJOR")
    field(HYST, "2")
    info (autosaveFields, "HIGH HIHI ADEL HYST")
}

