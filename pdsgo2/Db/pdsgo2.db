record (calc, "$(P)cnt-sum") {
    field(SCAN, "1 second")
    field(INPA, "$(P)cnt-1")
    field(INPB, "$(P)cnt-2")
    field(INPC, "$(P)cnt-3")
    field(INPD, "$(P)cnt-4")
    field(INPE, "$(P)cnt-5")
    field(INPF, "1")
    field(CALC, "(A+B+C+D+E)/F")
    field(EGU, "counts")
    field(PREC, "0")
    field(SDIS, "$(P)sDoseRt.VAL NPP MS")
    field(DISV, "-1") # Will never disable, but will propagate severity
}


record(stringin, "$(P)sErrStr") {
    field(PINI, "YES")
}
